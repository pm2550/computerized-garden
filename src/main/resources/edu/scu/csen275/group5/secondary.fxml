<VBox spacing="12" xmlns="http://javafx.com/javafx/13.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.scu.csen275.group5.SecondaryController">
    <padding>
        <Insets top="15" right="15" bottom="15" left="15"/>
    </padding>
    <Label text="Log Help / Manual" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
    <TextArea fx:id="helpArea" prefRowCount="12"/>
    <Button text="Return to Control Room" onAction="#switchToPrimary"/>
</VBox>
